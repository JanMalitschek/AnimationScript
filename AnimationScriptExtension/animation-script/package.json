{
    "name": "animation-script",
    "displayName": "Animation Script",
    "description": "Language support for Animation Script",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.66.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "as",
            "aliases": ["Animation Script", "as"],
            "extensions": [".as"],
            "configuration": "./language-configuration.json",
            "icon": {
                "dark": "icon.png",
                "light": "icon.png"
            }
        }],
        "grammars": [{
            "language": "as",
            "scopeName": "source.as",
            "path": "./syntaxes/as.tmLanguage.json"
        }]
    }
}